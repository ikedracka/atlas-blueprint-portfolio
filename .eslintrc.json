{
  "extends": ["next", "eslint:recommended", "plugin:import/recommended", "prettier"],
  "rules": {
    "import/no-unresolved": 0,
    "no-unsafe-optional-chaining": 0,
    "@next/next/no-script-in-document": "off",
    "import/order": "off"
  },
  "env": {
    "es6": true
  },
  "ignorePatterns": ["schema.generated.*"]
}
